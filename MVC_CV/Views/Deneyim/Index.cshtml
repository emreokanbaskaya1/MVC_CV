@using MVC_CV.Models.Entity
@model List<TblDeneyimler>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Deneyim Listesi</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Subtitle</th>
        <th>Date</th>
        <th>Delete</th>
        <th>Update</th>

    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.Baslik</td>
                    <td>@item.AltBaslik</td>
                    <td>@item.Tarih</td>
                    <td><a href="/Deneyim/DeneyimSil/@item.ID" class="btn btn-danger">Delete</a></td>
                    <td><a href="/Deneyim/DeneyimGetir/@item.ID" class="btn btn-warning">Update</a></td>
                </tr>
            }
</table>

<a href="/Deneyim/DeneyimEkle" class="btn btn-primary">Yeni Deneyim Ekle</a>

