@model MVC_CV.Models.Entity.TblSosyalMedya
@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Social Media Account Editing Page</h4>
<br />
<form method="post">

    @Html.Label("Social Media Name")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Ikon")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />

    <button class="btn btn-success">Update</button>

</form>




