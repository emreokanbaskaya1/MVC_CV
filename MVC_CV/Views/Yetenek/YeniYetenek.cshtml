@model MVC_CV.Models.Entity.TblYeteneklerim
@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>New Skill Adding Page</h4>
<br />
<form method="post">

    @Html.Label("Skill Description")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Rate")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
   
    <button class="btn btn-info">Save</button>

</form>
